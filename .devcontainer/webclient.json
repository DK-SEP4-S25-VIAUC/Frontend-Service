{
    "name": "WebClient Dev Container",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "webclient",
    "workspaceFolder": "/workspaces/Frontend-Service/WebClient",
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint"
            ]
        }
    },
    "postCreateCommand": "cd /workspaces/Frontend-Service/WebClient && npm install",
    "remoteUser": "node"
}