{
    "name": "WebApi Container",
    "dockerComposeFile": [
        "../../docker-compose.yml"
    ],
    "service": "webapi",
    "shutdownAction": "none",
    "workspaceFolder": "/workspace/WebApi",
    "customizations": {
        "vscode": {
            "extensions": [
                "esbenp.prettier-vscode",
                "dbaeumer.vscode-eslint",
                "ms-python.python",
                "ms-python.vscode-pylance"
            ],
            "settings": {
                "workbench.iconTheme": "material-icon-theme"
            }
        }
    }
}